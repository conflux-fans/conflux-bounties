{
  "name": "conflux-analytics-portal",
  "version": "1.0.0",
  "private": true,
  "description": "Conflux eSpace on-chain analytics portal with ingestion, API, and dashboard",
  "scripts": {
    "build": "pnpm -r build",
    "dev:api": "pnpm --filter @conflux-analytics/api dev",
    "dev:ingestor": "pnpm --filter @conflux-analytics/ingestor dev",
    "dev:frontend": "pnpm --filter @conflux-analytics/frontend dev",
    "test": "pnpm -r test",
    "test:integration": "node --experimental-vm-modules node_modules/.bin/jest --config jest.config.js --testPathPattern=integration",
    "test:e2e": "node --experimental-vm-modules node_modules/.bin/jest --config jest.config.js --testPathPattern=e2e",
    "lint": "pnpm -r lint",
    "migrate": "tsx scripts/migrate.ts",
    "seed": "tsx scripts/seed.ts",
    "backfill": "tsx scripts/backfill.ts",
    "generate-api-key": "tsx scripts/generate-api-key.ts",
    "docker:up": "docker compose up --build",
    "docker:down": "docker compose down -v"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.4.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.0"
  }
}
